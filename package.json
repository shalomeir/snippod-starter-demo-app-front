{
  "name": "snippod-starter-demo-app-front",
  "description": "A front part of the 'snippod-starter-demo-app' Full stack demo application based on React and Redux",
  "author": "Seonggyu Lee <shalomeir@gmail.com> (http://github.com/shalomeir)",
  "license": "MIT",
  "version": "2.2.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/shalomeir/snippod-starter-demo-app-front"
  },
  "homepage": "https://www.snippod.com",
  "keywords": [
    "react",
    "universal",
    "webpack",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel",
    "snippod",
    "demo",
    "webapplication",
    "fullstack",
    "semantic-ui"
  ],
  "main": "excute/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "concurrent --kill-others",
    "build-prod": "better-npm-run build-prod",
    "build-app": "gulp --cwd ./semantic build && webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "build:semantic": "gulp --cwd ./semantic build",
    "build:i18n": "rimraf _translations && babel --quiet --babelrc .babelrc.i18n src > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node scripts/i18nToXliff.js",
    "clean": "rimraf static/dist && gulp --cwd ./semantic clean",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev -- --nw",
    "start-dev-local": "better-npm-run start-dev-local -- --nw",
    "watch-client": "better-npm-run watch-client",
    "watch-client-local": "better-npm-run watch-client-local",
    "dev": "concurrent --kill-others \"gulp --cwd ./semantic watch-css\" \"npm run watch-client\" \"npm run start-dev\"",
    "dev-local": "concurrent --kill-others \"gulp --cwd ./semantic watch-css\" \"npm run watch-client-local\" \"npm run start-dev-local\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "build-prod": {
      "command": "npm run build-app",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIHOST": "snippod-demo-rest.ap-northeast-2.elasticbeanstalk.com"
      }
    },
    "start-prod": {
      "command": "node ./excute/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8081
      }
    },
    "start-dev": {
      "command": "node ./excute/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-dev-local": {
      "command": "node ./excute/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 8000,
        "APIHOST": "snippod-demo-rest.ap-northeast-2.elasticbeanstalk.com"
      }
    },
    "watch-client-local": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 8000,
        "APIHOST": "localhost"
      }
    }
  },
